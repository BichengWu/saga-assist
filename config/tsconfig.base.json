{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "sourceMap": true,
      "allowJs": false,
      "jsx": "react",
      "lib": ["esnext", "dom"],
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "suppressImplicitAnyIndexErrors": true,
      "noUnusedLocals": true,
      "downlevelIteration": true,
      "experimentalDecorators": true,
    },
  }